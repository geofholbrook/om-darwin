(in-package darwin)(defun create-genotype (template &key maxxed)  (translate-block template :maxxed maxxed))(defun translate-block (block &key maxxed)"recursive function that translates a (homogenous)lis or num or (mixed)list syntax of a genotype definition. see documentation ... once it has been written that is."  (if (and (atom block) block)     (break "invalid genotype definition")    (cond ((null block) nil)          ((equalp (first block) 'num)           (if maxxed             (fixed/apply (second block) '(second))             (fixed/rnd (second block))))          ((equalp (first block) 'lis)           (list-of (fixed/rnd (second block)) `(translate-block ,(third block) :maxxed ,maxxed)))          (t           (mapcar #'(lambda (b) (translate-block b :maxxed maxxed)) block)))))